<AnchorPane prefHeight="522.0" prefWidth="667.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ijse.gdse.project.controller.SettingController">
   <children>
      <ImageView fitHeight="522.0" fitWidth="667.0" pickOnBounds="true">
         <image>
            <Image url="@../images/pictures/Pane.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgProfile" fitHeight="53.0" fitWidth="78.0" layoutX="37.0" layoutY="125.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/pictures/Profile.jpg" />
         </image>
      </ImageView>
      <Circle fx:id="cir" fill="WHITE" layoutX="77.0" layoutY="149.0" radius="50.0" stroke="BLACK" strokeType="INSIDE" />
      <Label fx:id="lblName" layoutX="148.0" layoutY="115.0" text="Rishitha Wickramasinghe" textFill="#593f1e">
         <font>
            <Font name="Book Antiqua" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="txtName" layoutX="41.0" layoutY="286.0" prefHeight="31.0" prefWidth="304.0" promptText="Name" style="-fx-background-color: #ebebeb70; -fx-border-color: #000000; -fx-border-width: 0 0 1px 0;" />
      <Button layoutX="509.0" layoutY="286.0" mnemonicParsing="false" onAction="#changeNameOnAction" prefHeight="31.0" prefWidth="58.0" text="Change" />
      <TextField fx:id="txtUser" layoutX="41.0" layoutY="346.0" prefHeight="31.0" prefWidth="304.0" promptText="Username" style="-fx-background-color: #ebebeb70; -fx-border-color: #000000; -fx-border-width: 0 0 1px 0;" />
      <Button layoutX="509.0" layoutY="346.0" mnemonicParsing="false" onAction="#changeUserOnAction" prefHeight="31.0" prefWidth="58.0" text="Change" />
      <TextField fx:id="txtEmail" layoutX="41.0" layoutY="408.0" prefHeight="31.0" prefWidth="304.0" promptText="Email" style="-fx-background-color: #ebebeb70; -fx-border-color: #000000; -fx-border-width: 0 0 1px 0;" />
      <Button layoutX="509.0" layoutY="408.0" mnemonicParsing="false" onAction="#changeEmail" prefHeight="31.0" prefWidth="58.0" text="Change" />
      <Button layoutX="502.0" layoutY="467.0" mnemonicParsing="false" onAction="#confirmOnAction" prefHeight="31.0" prefWidth="65.0" text="Confirm" />
      <Button layoutX="593.0" layoutY="467.0" mnemonicParsing="false" onAction="#cancelOnAction" prefHeight="31.0" prefWidth="52.0" text="Cancel" />
      <Button layoutX="30.0" layoutY="209.0" mnemonicParsing="false" onAction="#changePhotoOnAction" prefHeight="31.0" prefWidth="93.0" text="Change Photo" />
      <Label layoutX="85.0" layoutY="22.0" text="Settings" textFill="#593f1e">
         <font>
            <Font name="Book Antiqua" size="30.0" />
         </font>
      </Label>
   </children>
